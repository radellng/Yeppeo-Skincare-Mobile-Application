{
  "expo": {
    "name": "yeppeo",
    "slug": "yeppeo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/yeppeo_icon.png",
    "splash": {
      "image": "./app/assets/yeppeo_splash.png",
      "height": 50,
      "width": 50,
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/yeppeo_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
